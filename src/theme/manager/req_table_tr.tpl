            <a class='mStep__step mStep__step--[{ STEP_3 }]' href="/manager/r/[{ ID }]/step/3">#3</a>
        </div>
    </td>
    <td class=''><a href="mailto:[{ EMAIL }]" title='Связаться с пользователем'>@</a> [{ AUTHOR_NAME }] [{ AUTHOR_LASTNAME }]</td>
    <td>[{ NICKNAME }] [{ NICK }] [{ /NICKNAME }]</td>
    <td class=''><a target='_blank' href="//vk.com/id[{ AUTHOR_VK_ID }]">[{ AUTHOR_VK_ID }]</a></td>
    <!-- <td class='tac'></td> -->
