    <td class='tac' title="[{ DATE_FULL }]">
        <span style="margin-right: 25px;">#[{ ID }]</span> [{ DATE }]
    </td>
    <td class='nop'>
        <div class="mStep flex">
            <a class='mStep__step mStep__step--[{ STEP_1 }]' href="/manager/r/[{ ID }]/step/1">#1</a>
