<tr data-req="[{ ID }]" href="/req/[{ ID }]/" onclick="window.location.href='/req/[{ ID }]/'" title="[{ NICK }]">
	<td class='tac'>[{ ID }]</td>
	<td class='tac'>[{ DATE }]</td>
	<!-- <td class='nick'><span>[{ NICK }]</span></td> -->
	<td data-step="[{ STEP_1 }]">[{ STEP_1 }]</td>
	<td data-step="[{ STEP_2 }]">[{ STEP_2 }]</td>
	<td data-step="[{ STEP_3 }]">[{ STEP_3 }]</td>
</tr>