<link rel="stylesheet" href="[{ THEME }]/styles/su.css?ver={ver=[{ THEME }]/styles/su.css}">
<script src="[{ THEME }]/js/su.js?ver={ver=[{ THEME }]/js/su.js}"></script>
<section class="SU">
	<div class="SU__wrap flex flex--aistr flex--jcs">
		<div class="SU__left">
			[{ SEARCH }]<div class="SU__leftSection SU__leftSection--search">
				[{ SEARCH }]
			</div>[{ /SEARCH }]
			[{ LASTRATES }]<div class="SU__leftSection SU__leftSection--lastrates">
				[{ LASTRATES }]
			</div>[{ /LASTRATES }]
			<div class="SU__leftSection SU__leftSection--usersinfo">
				[{ USERS_INFO }]
			</div>
			<div class="SU__leftSection SU__leftSection--reqsinfo">
				[{ REQS_INFO }]
			</div>
		</div>
		<div class="SU__right">
			[{ TITLE }]
			<div class="SU__Title">
				<h1>[{ TITLE }]</h1>
			</div>
			[{ /TITLE }]
			[{ CONTENT }]
		</div>
	</div>
</section>